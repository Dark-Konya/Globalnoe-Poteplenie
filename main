@bot.message_handler(commands=['facts'])
def send_welcome(text):
    message1 = bot.send_message(text.chat.id, """–ó–∞–≥—Ä—É–∑–∫–∞ ... """)
    time.sleep(2)
    bot.delete_message(text.chat.id, message1.id)
    bot.send_message(text.chat.id, random.choice(fakt) )

